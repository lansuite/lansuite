<?xml version="1.0" encoding="ISO-8859-15"?>
<lansuite>
<lansuite_header>
	<filetype>lansuite_import</filetype> <!-- contains the type of this file - must named lansuite_import-->
	<version>2.0</version> <!-- contains the lansuite version this file should be importet in -->
	<source>http://www.lansuite.de</source> <!-- contains where this file comes from (origin) -->
	<date>2003-05-15 16:16</date> <!-- contains the date when this file has been created (year - month - day - hour-minute)-->
</lansuite_header>


<!-- All data you want to import in lansuite is written between the importdata tags -->
<importdata>

<!-- The User-Tags contains all users that will be importeted into lansuite -->	

	<users>

		<!-- Each user starts with a user tag and ends with a user tag. All information about the user ist stored
		      between those user tags -->
		      	
		<user>
			<email>raphael@one-network.org</email>  <!-- the users email-address LOGIN-->
			<username>raphael</username> 	  <!-- the users nickname -->
			<name>Mansmann</name> 			  <!-- the users name for example Backes -->
			<firstname>Raphael</firstname> 	  <!-- the users first name -->
			<type>3</type>  			  <!-- the users kind: 1 - guest, 2 - admin, 3 - operator -->
			<paid>0</paid>				<!-- only needed if type == 1 - 0 means user don't have paid, 1 means user had paid-->
			<password>098f6bcd4621d373cade4e832627b4f6</password> 	  <!-- the users password encrypted in MD5 -->
			<clan>lansuite</clan>  			  <!-- OPTIONAL --><!-- the users clan name -->
			<wwclid></wwclid>  			  <!-- OPTIONAL --><!-- the users WWCL-identificationnumber -->
			<wwclclanid></wwclclanid>  		  <!-- OPTIONAL --><!-- the users clan WWCL-identificationnumber -->
		</user>
		
		
		<!-- some other examples: -->
				
		<user>
			<email>admin@lansuite.de</email> 
			<username>admin</username> 	  	
			<name>admin</name> 			
			<firstname>admin</firstname> 	  	
			<type>2</type>  			
			<password>098f6bcd4621d373cade4e832627b4f6</password> 	
			<clan>lansuite</clan>  			
			<wwclid></wwclid>  			
			<wwclclanid></wwclclanid>  		
		</user>
		
		<user>
			<email>user1</email> 
			<username>user1</username> 	  	
			<name>user1</name> 			
			<firstname>user1</firstname> 	  	
			<type>1</type>
			<paid>1</paid>			
			<password>098f6bcd4621d373cade4e832627b4f6</password> 	
			<clan>lala</clan>  			
			<wwclid></wwclid>  			
			<wwclclanid></wwclclanid>  		
		</user>
		
	</users>
	
	<seat_blocks>
	
	<!-- each seatblock starts with "block" and ends with "block"-->
		
		<block>
			<rows>10</rows><!-- means horizontal length (number of rows)-->
 			<cols>10</cols><!-- means vertical length (number of cols)-->
 			<name>Testblock</name>
 			<orientation>1</orientation><!-- 1 orientation is horizontal - 0 - vertical -->
 			<remark></remark>   <!-- OPTIONAL -->
 			<text_tl></text_tl> <!-- OPTIONAL -->
 			<text_tc></text_tc> <!-- OPTIONAL -->
 			<text_tr></text_tr> <!-- OPTIONAL -->
 			<text_lt></text_lt> <!-- OPTIONAL -->
 			<text_lc></text_lc> <!-- OPTIONAL -->
 			<text_lb></text_lb> <!-- OPTIONAL -->
 			<text_rt></text_rt> <!-- OPTIONAL -->
 			<text_rc></text_rc> <!-- OPTIONAL -->
 			<text_rb></text_rb> <!-- OPTIONAL -->
 			<text_bl></text_bl> <!-- OPTIONAL -->
 			<text_bc></text_bc> <!-- OPTIONAL -->
 			<text_br></text_br> <!-- OPTIONAL -->
		
		
			<seat_seats>
			<!-- All seats in the current block -->		
				<seat>
					<col>2</col><!-- the coordinates of the seat, other than de length this coordinates begin with 0 (this user seat is in row 3)-->
					<row>5</row><!-- look above -->
					<status>2</status><!-- 1 free, 2 occupied -->
					<owner>christian@one-network.org</owner><!-- instead seatid there must be the email as unique identifier -->
					<ipaddress>10.0.30.1</ipaddress><!-- IP-Address related to this seat OPTIONAL-->
				</seat>
				<seat>
					<col>3</col>
					<row>5</row>
					<status>2</status>
					<owner>admin@lansuite.de</owner>
					<ipaddress>10.0.30.2</ipaddress>
				</seat>

			</seat_seats>
		
			<seat_sep>
			<!-- All seperators in the current block -->				
				<sep>
					<orientation>1</orientation><!-- shows which direction the seperator is -->
					<value>2</value><!-- value of the row or col after that the seperator will be included -->
				</sep>
				
			</seat_sep>		
		
		</block>
	
	</seat_blocks>

</importdata>
</lansuite>
